app: code
-
# VS Code commands
save file: key(ctrl-s)
open file: key(ctrl-o)
find: key(ctrl-f)
replace: key(ctrl-h)
go to line: key(ctrl-g)

# Emoji insertion
insert {user.emoji}: "{user.emoji}"

# Key commands
press {user.keys}: key({user.keys})

# Code snippets
say {user.snippets}: 
    insert(user.snippets)